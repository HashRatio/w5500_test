# Global controls
NET "ex_clk_i" LOC = J16;
NET "ex_clk_o" LOC = F6;

#NET "INT" LOC = R7;

# UART
NET "uart1SIN"  LOC = E7;
NET "uart1SOUT" LOC = C7;
NET "uart1SOUT" PULLUP;

NET "uart2SIN"  LOC = A9;
NET "uart2SOUT" LOC = D8;
NET "uart2SOUT" PULLUP;

NET "uart3SIN"  LOC = L1;
NET "uart3SOUT" LOC = M1;
NET "uart3SOUT" PULLUP;


NET "uart4SIN"  LOC = K2;
NET "uart4SOUT" LOC = K1;
NET "uart4SOUT" PULLUP;

# UART debug
NET "uart_debugSIN" LOC = T15;
NET "uart_debugSOUT" LOC = R15;
NET "uart_debugSOUT" PULLUP;

NET "w5500_spi_MISO_MASTER" LOC = A5;
NET "w5500_spi_MOSI_MASTER" LOC = F7;
NET "w5500_spi_SS_N_MASTER" LOC = D6;
NET "w5500_spi_SCLK_MASTER" LOC = C6;
NET "w5500_spi_RESET" LOC = A4;
NET "w5500_INT" LOC = A6;

NET "flash_spi_MISO_MASTER" LOC = G1;
NET "flash_spi_MOSI_MASTER" LOC = M2;
NET "flash_spi_SS_N_MASTER" LOC = F1;
NET "flash_spi_SCLK_MASTER" LOC = J1;
#NET "w5500_spi_RESET" LOC = A4; 
#NET "w5500_INT" LOC = A6;

NET "INT" LOC = F2;

# SPI Flash: W25Q80BV
#NET "POWER_ON" LOC = T3 | SLEW = QUIETIO;

#NET "*" IOSTANDARD = LVCMOS33;

#LED
#NET "LED[0]" LOC = F4; 
#NET "LED[1]" LOC = F5;
#NET "LED[2]" LOC = F6;
#NET "LED[3]" LOC = F7;
#NET "LED[4]" LOC = B15;
#NET "LED[5]" LOC = B16;
#NET "LED[6]" LOC = C15;
#NET "LED[7]" LOC = C16;

NET "ex_clk_i" TNM_NET = "ex_clk_i";
TIMESPEC TS_clk = PERIOD "ex_clk_i" 40 ns HIGH 50 %;

NET "clk_i" TNM_NET = "clk_i";
TIMESPEC TS_clk_i = PERIOD "clk_i" 10 ns HIGH 50 %;
